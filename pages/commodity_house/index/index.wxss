view {
  overflow: visible;
  word-break: normal;
}

.container {
  padding: 0px;
  background: #f7f7f7;
}

.row {
  margin: 30rpx;
  width: 100%;
  margin-top: 0rpx;
}

.row .boxs {
  background: #fff;
  padding: 20rpx 0;
}

.row .box {
  margin: 0rpx 30rpx;
}

.row .title {
  margin-bottom: 20rpx;
}

.row2 .box {
  margin: 0rpx 30rpx;
  border-bottom: 1rpx solid #eee;
  line-height: 60rpx;
  height: 60rpx;
}

.row1 {
  width: 100%;
  margin: 0rpx;
  position: relative;
}

.row1 swiper {
  height: 100%;
}

.row1 image {
  width: 100%;
}

.row1 .indicator {
  color: #fff;
  position: absolute;
  bottom: 0rpx;
  right: 10rpx;
}

.row2 {
  width: 100%;
}

.row2 .box1 {
  font-size: 40rpx;
  border: 0px;
}

.row2 .box2 {
  color: #888;
  font-size: 30rpx;
}

.row2 .box .fl {
  float: left;
}

.row2 .box2 .fl text {
  font-size: 32rpx;
  color: #ffb160;
}

.row2 .box .fr {
  float: right;
}

.row2 .detail .fl {
  width: 25%;
  color: #bbb;
  font-size: 24rpx;
}

.row2 .detail .fr {
  width: 75%;
  color: #0f0f0f;
  font-size: 24rpx;
}

.row2 .detail30 {
  color: #bbb;
  font-size: 24rpx;
  text-align: center;
}

.row2 .reg image {
  width: 40rpx;
  height: 40rpx;
  margin-top: 10rpx;
  float: left;
  margin-left: 50%;
  left: -150rpx;
  position: relative;
}

.row2 .reg text {
  float: left;
  position: relative;
  left: -140rpx;
}

.row2 .reg {
  text-align: center;
  font-size: 24rpx;
  color: #ffcf9b;
  line-height: 60rpx;
}

.row2 .box2 {
  border: 0px;
}

.row2 .box3 {
  border: 0px;
  color: red;
  font-size: 24rpx;
  line-height: 30rpx;
}

.row3 {
  width: 100%;
  margin-top: 0rpx;
}

.row3 .boxs {
  padding: 20rpx 0;
}

.row3 .box {
  margin: 0rpx 30rpx;
}

.row .title {
  font-size: 32rpx;
  color: #0f0f0f;
  margin-bottom: 20rpx;
}

.row3 .box2 {
  display: block;
}

.row3 .box2 .wxParse-image {
  width: 100%;
}

.row3 .box2 .wxParse-p {
  margin-bottom: 20rpx;
  line-height: 20rpx;
}

.row4 {
  width: 100%;
  margin-top: 0rpx;
}

.row4 .boxs {
  padding: 20rpx 0;
}

.row4 .box {
  margin: 0rpx 30rpx;
}

.row4 .title {
  margin-bottom: 20rpx;
}

.row5 swiper {
  height: 100px;
}

.row5 .type-page {
  height: 200rpx;
  padding-bottom: 10rpx;
}

.row5 .type-item {
  position: relative;
  display: inline-block;
  width: 230rpx;
}

.row5 .type-item-img {
  display: inline-block;
  width: 200rpx;
  height: 200rpx;
}

.row5 .type-item-name, .row5 .type-item-bg {
  position: absolute;
  bottom: 4rpx;
  left: 0;
  width: 200rpx;
  height: 30rpx;
  padding-top: 5rpx;
  padding-bottom: 5rpx;
  z-index: 99;
  font-size: 15px;
  text-align: center;
}

.row5 .type-item-bg {
  z-index: 19;
  background-color: #000;
  opacity: 0.5;
}

.row5 .type-item-name {
  z-index: 20;
  color: #fff;
}

.row6 {
  height: 330rpx;
  overflow: hidden;
  padding-bottom: 10rpx;
  color: #666;
  font-size: 14px;
}

.row6 .room {
  display: inline-block;
  width: 230rpx;
  height: 270rpx;
  text-align: center;
}

.row6 image {
  width: 230rpx;
  height: 170rpx;
}

.row7 {
  position: fixed;
  bottom: -30rpx;
  background: #fff;
  left: -30rpx;
  height: 80rpx;
  line-height: 80rpx;
}

.row7 .boxs {
  margin: 0px;
  padding: 0px;
  height: 100%;
  font-size: 40rpx;
}

.row7 .boxs .box {
  display: inline-block;
  width: 50%;
  text-align: center;
  margin: 0px;
  padding: 0px;
  height: 100%;
}

.row7 .box1, .row7 .box2 {
  display: inline-block;
  width: 50%;
  text-align: center;
  height: 100%;
  color: #fff;
}

.row7 .box1 {
  background-color: #ffb160;
}

.row7 .box2 {
  background-color: #ff4848;
}

.row8 {
  height: 60rpx;
}
